{
  "name": "tree-meh",
  "displayName": "Tree Meh Extension",
  "description": "Adds right click option to copy tree output to clipboard on selected directory from navigation pane.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
        {
            "command": "tree-meh.generateTree",
            "title": "Generate Directory Tree"
        }
    ],
    "menus": {
        "explorer/context": [
            {
                "when": "resourceScheme == 'file'",
                "command": "tree-meh.generateTree",
                "group": "tree-meh"
            }
        ]
    }
},
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
